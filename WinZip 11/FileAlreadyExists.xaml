<mica:MicaWindow x:Class="WinZip_11.FileAlreadyExists"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WinZip_11"
        xmlns:mica="clr-namespace:MicaWPF.Controls;assembly=MicaWPF"
        xmlns:wpfui="http://schemas.lepo.co/wpfui/2022/xaml"
        mc:Ignorable="d"
        Title="FileAlreadyExists" Height="168" Width="377">
    <Grid>
        <Label Content="Looks Like The Folder is already exists." FontFamily="Segoe UI Variable Display" FontSize="13" HorizontalAlignment="Left" Margin="21,18,0,0" VerticalAlignment="Top" Width="346"/>
        <Label Content="Do you want to open folder?" HorizontalAlignment="Left" FontFamily="Segoe UI Variable Display" FontSize="13" Margin="21,38,0,0" VerticalAlignment="Top" Width="346"/>
        <Label Content="" x:Name="FolderPath" HorizontalAlignment="Left" FontFamily="Segoe UI Variable Display" FontSize="13" Margin="21,0,0,0" VerticalAlignment="Center" Width="346"/>
        <wpfui:Button Content="Yes" Margin="21,93,212,0" VerticalAlignment="Top" Appearance="Secondary" Click="Button_Click"/>
        <wpfui:Button Content="No" Margin="216,93,19,0" VerticalAlignment="Top" Appearance="Secondary"/>
    </Grid>
</mica:MicaWindow>
